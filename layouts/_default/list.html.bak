{{ define "main" }}

{{ $products := slice "exterior-window-sticker-blank" "exterior-window-sticker-custom" }}

{{ range $index, $productID := $products }}
{{ $product := index site.Data.products $productID }}

<section id="{{ $product.id }}-section" class="{{ if eq $index 0 }}active{{ end }}">
	<div id="container16" class="style5 container columns">
		<div class="wrapper">
			<div class="inner">
				<div>
					<h2 id="text10" class="style5" style="opacity: 1; transform: none;">{{ $product.title | safeHTML }}</h2>
					<div id="{{ $product.id }}-slideshow" class="style1 productSlider slideshow" oncontextmenu="return false;"
						ondragstart="return false;">
						<div class="content">
							<div class="bg slideshow-background fade">
								{{ range $imgIndex, $image := $product.images }}
								<div role="img" aria-label="{{ $image.alt }}" class="{{ if eq $imgIndex 0 }}visible top initial{{ end }}"
									style="background-image: url(&quot;{{ $image.src }}&quot;); background-position: center center; background-repeat: no-repeat; background-size: contain;">
								</div>
								{{ end }}
							</div>
							<div class="nav next"></div>
							<div class="nav previous"></div>
						</div>
					</div>
					<h2 id="text23" class="style3" style="opacity: 1; transform: none;"><strong>${{ $product.price }}</strong>* {{ $product.price_note }}</h2>
					<ul id="buttons10" class="style5 buttons">
						{{ range $linkIndex, $link := $product.purchase_links }}
						<li style="opacity: 1;">
							<a href="{{ $link.url }}" class="button n0{{ add $linkIndex 1 }}"
								role="button"><svg aria-labelledby="buttons10-icon-{{ add $linkIndex 1 }}-title">
									<title id="buttons10-icon-{{ add $linkIndex 1 }}-title">{{ $link.icon | title }}</title>
									<use xlink:href="{{ "/icons.svg#" | relURL }}{{ $link.icon }}"></use>
								</svg><span class="label">{{ if $link.primary }}*{{ end }} {{ $link.name }}</span></a>
						</li>
						{{ end }}
					</ul>
					<ul id="links04" class="style2 links">
						<li class="n01">
							<a href="#exterior-blank">More details</a>
						</li>
					</ul>
				</div>
				<div>
					<h2 id="text38" class="style5" style="opacity: 1; transform: none;">Exterior Window Sticker
						<strong>Custom</strong> <span style="color: #bbbbbb">â€“&nbsp;8.5&nbsp;x&nbsp;11</span></h2>
					<div class="style1 productSlider slideshow" oncontextmenu="return false;"
						ondragstart="return false;">
						<div class="content">
							<div class="bg slideshow-background fade">
								<div role="img" aria-label="Exterior Window Sticker Custom" class="visible top initial"
									style="background-image: url(&quot;/images/slideshow06-0cd21dcf.jpg&quot;); background-position: center center; background-repeat: no-repeat; background-size: contain;">
								</div>
								<div role="img" aria-label="Exterior Window Sticker Custom"
									style="background-image: url(&quot;/images/slideshow06-f74c3c8c.jpg&quot;); background-position: center center; background-repeat: no-repeat; background-size: contain;">
								</div>
								<div role="img" aria-label="Exterior Window Sticker Custom"
									style="background-image: url(&quot;/images/slideshow06-40899e5c.jpg&quot;); background-position: center center; background-repeat: no-repeat; background-size: contain;">
								</div>
							</div>
							<div class="nav next"></div>
							<div class="nav previous"></div>
						</div>
					</div>
					<h2 id="text11" class="style3" style="opacity: 1; transform: none;"><strong>$0.54</strong>* per blank (min
						1000)</h2>
					<ul id="buttons08" class="style5 buttons">
						<li style="opacity: 1;">
							<a href="https://akdealerservices.com/product/1000535-exterior-window-sticker-blank" class="button n01"
								role="button"><svg aria-labelledby="buttons08-icon-1-title">
									<title id="buttons08-icon-1-title">Cart</title>
									<use xlink:href="{{ "/icons.svg#cart" | relURL }}"></use>
								</svg><span class="label">* Buy on AK Dealer Services</span></a>
						</li>
						<li style="opacity: 1;">
							<a href="#" class="button n02" role="button"><svg aria-labelledby="buttons08-icon-2-title">
									<title id="buttons08-icon-2-title">Cart</title>
									<use xlink:href="{{ "/icons.svg#cart" | relURL }}"></use>
								</svg><span class="label">Buy on Amazon</span></a>
						</li>
						<li style="opacity: 1;">
							<a href="#wholesale" class="button n03" role="button"><svg aria-labelledby="buttons08-icon-3-title">
									<title id="buttons08-icon-3-title">Cart</title>
									<use xlink:href="{{ "/icons.svg#cart" | relURL }}"></use>
								</svg><span class="label">Wholesale Inquiry</span></a>
						</li>
					</ul>
					<ul id="links01" class="style2 links">
						<li class="n01">
							<a href="#exterior-custom">More details</a>
						</li>
					</ul>
				</div>
			</div>
		</div>
	</div>
</section>

{{ end }}
