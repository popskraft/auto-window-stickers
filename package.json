{
  "name": "nutcracker",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "hugo server --disableFastRender",
    "build": "hugo --gc --minify",
    "tailwind:watch": "npx tailwindcss -i ./assets/tailwind/src/base.css -o ./assets/tailwindstyle.css --watch --config ./assets/tailwind/tailwind.config.js",
    "tailwind:build": "npx tailwindcss -i ./assets/tailwind/src/base.css -o ./assets/tailwindstyle.css --minify --config ./assets/tailwind/tailwind.config.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run tailwind:watch\"",
    "generate:content": "node scripts/content-generator.js",
    "generate:content:dry": "node scripts/content-generator.js --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/popskraft/nutcracker.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/popskraft/nutcracker/issues"
  },
  "homepage": "https://github.com/popskraft/nutcracker#readme",
  "description": "",
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.1.2",
    "postcss": "^8.4.21",
    "postcss-cli": "^11.0.1",
    "tailwindcss": "^3.3.0"
  }
}
